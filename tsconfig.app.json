{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
    "types": [
      "node"
    ]
  },
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "server/**/*.ts",
    "api"
  ],
  "exclude": [
    "src/**/__tests__/*"
  ]
}
